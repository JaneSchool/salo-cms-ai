{
  "name": "salo-cms-ai",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "bun@1.1.29",
  "private": true,
  "license": "Apache-2.0",
  "keywords": ["cms", "biomejs", "bun", "typescript", "vue", "tailwindcss", "astrojs", "react", "shadcn"],
  "author": "Hrihorii Ilin <devopsick@pm.me>",
  "description": "Content Managment System With Piggy AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/Frikadellios/salo-cms-ai.git"
  },
  "bugs": {
    "url": "https://github.com/Frikadellios/salo-cms-ai/issues"
  },
  "homepage": "https://github.com/Frikadellios/salo-cms-ai#readme",
  "engines": {
    "node": ">=18.19.1 <19 || >=20.17.0 <21 || >=21.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "browserslist": "bunx update-browserslist-db@latest",
    "tailwindcss": "bunx @tailwindcss/cli@next -i ./src/styles/app.css -o ./src/styles/index.css",
    "dev": "bun run tailwindcss -m && bunx --bun astro dev --verbose",
    "start": "bun run cf-typegen && bun run astro check --verbose && bunx --bun astro dev --verbose",
    "build": "bunx --bun astro build --verbose ./dist",
    "preview": "bunx --bun astro build ./dist && bun run wrangler pages dev ./dist",
    "astro": "astro",
    "cf-typegen": "bun run wrangler types",
    "deploy": "bun run build && bun run wrangler pages deploy ./dist",
    "lint": "conc bun:lint:*",
    "lint:format": "bunx @biomejs/biome format . --write",
    "lint:fix": "bunx @biomejs/biome check . --fix",
    "lint:check": "bunx @biomejs/biome check ./* --write --unsafe",
    "inspect": "bunx serve .vite-inspect"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/react": "^3.6.2",
    "@astrojs/ts-plugin": "^1.10.3",
    "@astrojs/vue": "^4.5.2",
    "@csstools/postcss-oklab-function": "^4.0.3",
    "@radix-icons/vue": "^1.0.0",
    "@tailwindcss/cli": "^4.0.0-alpha.28",
    "@tailwindcss/node": "^4.0.0-alpha.28",
    "@tailwindcss/postcss": "^4.0.0-alpha.28",
    "@tailwindcss/vite": "^4.0.0-alpha.28",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vueuse/core": "^11.1.0",
    "astro": "^4.16.6",
    "caniuse-lite": "^1.0.30001669",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "lucide-vue-next": "^0.453.0",
    "radix-vue": "^1.9.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^4.0.0-alpha.28",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-sonner": "^1.2.2",
    "vue-tsc": "^2.1.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20241018.0",
    "@types/bun": "latest",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.7.7",
    "@types/web": "^0.0.174",
    "@vitejs/plugin-legacy": "^5.4.2",
    "autoprefixer": "^10.4.20",
    "bun-devtools": "latest",
    "bun-types": "latest",
    "editorconfig-checker": "^6.0.0",
    "postcss": "^8.4.47",
    "postcss-pxtorem": "^6.1.0",
    "postcss-cli": "^11.0.0",
    "postcss-discard-comments": "^7.0.3",
    "postcss-lightningcss": "^1.0.1",
    "postcss-preset-env": "^10.0.7",
    "terser": "^5.36.0",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-lightningcss": "^0.0.5",
    "wrangler": "^3.81.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": ["@biomejs/biome", "core-js", "esbuild", "sharp", "vue-demi", "workerd"]
}
