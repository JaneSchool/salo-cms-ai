  "engines": {
    "node": ">=18.19.1 <19 || >=20.17.0 <21 || >=21.1.0"
  },
  "scripts": {
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun run cf-typegen && bun run astro check --verbose && bunx --bun astro dev --verbose",
    "build": "bun run astro check --verbose && bunx --bun astro build --verbose ./dist",
    "preview": "bunx --bun astro build ./dist && bun run wrangler pages dev ./dist",
    "astro": "astro",
    "deploy": "bunx --bun astro build -- --force ./dist && wbun run wrangler pages deploy ./dist",
    "cf-typegen": "bun run wrangler types"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
