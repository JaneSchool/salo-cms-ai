---
import App from '@/components/vue/App.vue'
---

<App client:load />

<script is:inline lang="ts">
  import { createApp } from "vue";
  import router from "./app"; // Import the router

  // Create and mount the Vue app with router
  if (typeof window !== "undefined") {
    const app = createApp({});
    app.use(router);
    app.mount("#app");
  }
</script>
